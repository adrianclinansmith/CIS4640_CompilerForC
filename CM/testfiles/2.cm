int max;
void fib(int a, int b) {
    if (max < 10) {
        output(b);
        max = max + 1;
        fib(b, a+b);
    }
}
void main(void) {
    int a; int b;
    max = 0;
    a = input();
    b = input();
    output(a);
    fib(a, b);

}