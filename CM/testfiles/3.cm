void inputArray(int a[], int size) {
    int i;
    i = 0;
    while (i < size) {
        a[i] = input();
        i = i + 1;
    }
}
void main(void) {
    int x[5]; int n; int i;
    i = 0;
    n = i;
    inputArray(x, 5);
    while (i < 5) {
        n = n + x[i];
        i = i + 1;
    } 
    output(n/5);
}